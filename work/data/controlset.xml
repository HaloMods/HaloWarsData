<?xml version="1.0" encoding="us-ascii"?>
<Controls>
	<Mode name="Root">
		<Control name="KeyAccent" command="configToggle(&quot;consoleRenderEnable&quot;)" event="start" />
		<Control name="KeyTab" command="cycleConsoleRenderMode(true)" event="start" />
	</Mode>
	<Mode name="World">
		<Control name="KeyL" command="upgradeLevel()" event="start" />
		<Control name="KeyF8" command="saveGame" event="start" />
		<Control name="KeyF9" command="loadGame" event="start" />
	</Mode>
	<Mode name="Game" />
	<Mode name="Normal" />
</Controls>